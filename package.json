{
  "name": "component-template",
  "description": "A React component template for building and publishing reactstrap components",
  "version": "0.2.0",
  "homepage": "https://reactstrap.github.io/component-template",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/reactstrap/component-template.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "prebuild": "babel src/components --out-dir lib --ignore test.js",
    "create-release-branch": "sh ./scripts/create-release-branch",
    "publish-release": "sh ./scripts/publish-release",
    "deploy-docs": "sh ./scripts/docs",
    "ci-deploy-docs": "sh ./scripts/docs-ci",
    "gh-pages": "REACT_APP_GH_PAGES_PATH='component-template' npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "bootstrap": "^4.0.0-alpha.4",
    "conventional-changelog-cli": "^1.1.1",
    "conventional-recommended-bump": "^0.3.0",
    "dentist": "^1.0.3",
    "enzyme": "^2.4.1",
    "history": "^4.2.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^4.0.0-alpha.3",
    "react-scripts": "0.6.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "json-beautify": "^1.0.1",
    "keycode": "^2.1.7",
    "react-fontawesome": "^1.4.0",
    "slate": "^0.16.1"
  },
  "peerDependencies": {
    "reactstrap": "^2.0.0 || ^3.0.0"
  },
  "license": "MIT",
  "files": [
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "lib"
  ],
  "keywords": [
    "component-template",
    "react",
    "component",
    "components",
    "react-component",
    "reactstrap-component",
    "bootstrap"
  ]
}
